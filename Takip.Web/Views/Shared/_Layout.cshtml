    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - Takip.Web</title>
    <link rel="stylesheet" href="~/lib/bootstrap/main.css" />
